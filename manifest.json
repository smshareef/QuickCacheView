{
  "manifest_version": 3,
  "name": "QuickCacheView",
  "version": "1.0",
  "description": "Get the web page text quickly and easily from Google web cache.",
  "permissions": ["activeTab"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
